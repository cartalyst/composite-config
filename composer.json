{
    "name": "cartalyst/composite-config",
    "description": "Override illuminate/config filesystem configuration in a database.",
    "keywords": [
        "platform",
        "ecommerce",
        "laravel",
        "bootstrap",
        "config"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": "~5.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Cartalyst\\CompositeConfig\\": "src"
        }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}

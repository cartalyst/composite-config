{
	"name": "cartalyst/composite-config",
	"description": "Composite configuration allowing you to override filesystem configuration in a database.",
	"keywords": ["platform", "ecommerce", "laravel", "bootstrap"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Cartalyst LLC",
			"email": "ben.corlett@cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.3.0",

		"illuminate/config": "4.0.*",
		"illuminate/database": "4.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.7.2"
	},
	"autoload": {
		"classmap": [
			"src/migrations"
		],
		"psr-0": {
			"Cartalyst\\CompositeConfig": "src"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"minimum-stability": "dev"
}
